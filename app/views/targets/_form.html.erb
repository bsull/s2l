<%= form_for [@targetable, @target] do |f| %>
  <div class='row'>
    <%= f.label :fiscal_year, 'FY Ending ' + Date::MONTHNAMES.at(@current_account.fiscal_year_end) %>
    <%= f.text_field :fiscal_year %>
    <br><span class="hint">Enter a year, like 2020.</span>
  </div>
  <div class='row'><%= f.label :q1 %><%= f.text_field :q1 %></div>
  <div class='row'><%= f.label :q2 %><%= f.text_field :q2 %></div>
  <div class='row'><%= f.label :q3 %><%= f.text_field :q3 %></div>
  <div class='row'><%= f.label :q4 %><%= f.text_field :q4 %></div>
  <div class='row'><%= f.submit %></div>
<% end %>	
  
  

  
   

  
  

  
  

  
  

