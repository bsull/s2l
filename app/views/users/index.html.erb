<h1>Users</h1>
<table>
  <tr>
    <th>Nickname</th>
    <th>Email</th>
    <th>Role</th>
  </tr>
  <% @users.each do |user| %>
  <tr>	
    <td><%= link_to user.nickname, user %></td>
    <td><%= user.email %></td>
    <td><%= user.role %></td>
    <% if can? :manage, User %>
      <td><%= link_to 'edit', edit_user_path(user) %></td>
    <% end %>
  </tr>
<% end %>
</table>
<%= link_to 'New User', new_user_path %>
